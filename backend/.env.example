# --- Database Configuration ---
# Your database host (e.g., localhost, 127.0.0.1, or a remote IP)
DB_HOST=localhost

# The name of your database
DB_NAME=your_db_name

# The username for your database
DB_USER=your_db_user

# The password for your database user
DB_PASS=your_db_password

# --- Telegram Bot Configuration ---
# Your Telegram bot token from BotFather
TELEGRAM_BOT_TOKEN=123456789:ABCdefGHIjklMNOpqrSTUvwxYZ

# The ID of the channel the bot should listen to for results
# This is usually a negative number for channels.
TELEGRAM_CHANNEL_ID=-1001234567890

# Your personal Telegram User ID to access admin commands
TELEGRAM_SUPER_ADMIN_ID=123456789
